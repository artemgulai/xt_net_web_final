@if (IsAjax)
{
    string type = Request["type"];

    switch (type)
    {
        case "deleteAdmin":
            {
                int id = int.Parse(Request["id"]);
                EPAM.VacancyPortal.PL.WebPL.Models.AdminRequestHandler.Delete(id);
                break;
            }
        case "verifyAdmin":
            {
                int id = int.Parse(Request["id"]);
                EPAM.VacancyPortal.PL.WebPL.Models.AdminRequestHandler.Verify(id);
                break;
            }
        case "deleteEmployee":
            {
                EPAM.VacancyPortal.PL.WebPL.Models.EmployeeRequestHandler.Delete(Request,Response);
                break;
            }
            case "deleteEmployer":
            {
                EPAM.VacancyPortal.PL.WebPL.Models.EmployerRequestHandler.DeleteRequest(Request,Response);
                break;
            }
    }
}