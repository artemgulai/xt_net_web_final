@if (IsAjax)
{
    var type = Request["type"];

    switch (type)
    {
        case "editProfile":
            {
                EPAM.VacancyPortal.PL.WebPL.Models.EmployeeRequestHandler.Update(Request,Response);
                break;
            }
        case "deleteProfile":
            {
                EPAM.VacancyPortal.PL.WebPL.Models.EmployeeRequestHandler.Delete(Request,Response);
                FormsAuthentication.SignOut();
                break;
            }
        case "updateSkillLevel":
            {
                EPAM.VacancyPortal.PL.WebPL.Models.EmployeeRequestHandler.UpdateSkillLevel(Request,Response);
                break;
            }
        case "deleteSkill":
            {
                EPAM.VacancyPortal.PL.WebPL.Models.EmployeeRequestHandler.DeleteSkill(Request,Response);
                break;
            }
    }
}